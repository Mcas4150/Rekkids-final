<%= simple_form_for(@record) do |f| %>
    <%= f.input :name, placeholder: "title" %>
    <%= f.input :artist %>
    <%= f.input :price %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :photo, as: :attachinary %>
    <%= f.button :submit, class: "btn btn-primary"  %>
<% end %>
